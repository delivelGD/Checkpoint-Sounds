{
	"geode": "4.8.0",
	"gd": {
		"win": "2.2074",
		"mac": "2.2074",
		"android": "2.2074",
		"ios": "2.2074"

	},
	"id": "delivel.checkpoint_sounds",
	"name": "Checkpoint Sounds",
	"version": "v1.1.0",
	"developer": "delivel",
	"description": "Sounds for checkpoinds!",
	"tags": [
		"offline",
		"music",
		"utility",
		"universal"
	],
	"resources": {
		"files": [
			"resources/checkpointSound.ogg",
			"resources/checkpointSound1.ogg"
		]
	},
	"links": {
		"source": "https://github.com/delivelGD/Checkpoint-Sounds"
	},
	"settings": {
		"customization-title": {
			"type": "title",
			"name": "Customization"		
		},
		"enable-creating-sounds": {
			"type": "bool",
			"name": "Enable sounds of checkpoints creation",
			"default": true
		},
		"enable-deleting-sounds": {
			"type": "bool",
			"name": "Enable sounds of checkpoints deletion",
			"default": true
		},
		"choose-custom-sound-for-creating": {
			"type": "file",
			"name": "Choose custom sound for creating checkpoints",
			"default": "YourSound",
			"control": {
				"dialog": "open",
				"filters": [
					{
						"files": ["*.ogg", "*.mp3", "*.wav"],
						"description": "OGG/MP3/WAV files"
					}
				]
			}
		},
		"choose-custom-sound-for-deleting": {
			"type": "file",
			"name": "Choose custom sound for deleting checkpoints",
			"default": "YourSound",
			"control": {
				"dialog": "open",
				"filters": [
					{
						"files": ["*.ogg", "*.mp3", "*.wav"],
						"description": "OGG/MP3/WAV files"
					}
				]
			}
		}
	}
}
